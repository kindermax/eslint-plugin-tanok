{
  "name": "eslint-plugin-tanok",
  "version": "0.1.1",
  "description": "Eslint plugin for tanok.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "tanok"
  ],
  "author": "Kindritskiy Maksym",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kindritskyiMax/eslint-plugin-tanok.git"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "~3.9.1",
    "jest": "^23.4.2",
    "jest-runner-eslint": "^0.6.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "verbose": true,
    "projects": [
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/**/__tests__/*.js"
        ]
      }
    ]
  },
  "license": "MIT"
}
